# Environment Variables Example
# Copy this file to .env and customize as needed
# Usage: These values are loaded by pydantic-settings in app/config.py

# API Configuration
APP_NAME=Object Detection API
APP_VERSION=1.0.0

# Model Configuration
MODEL_NAME=yolov8n.pt
CONFIDENCE_THRESHOLD=0.5
IOU_THRESHOLD=0.45

# File Upload Limits
MAX_FILE_SIZE=10000000  # 10MB in bytes

# Server Configuration
HOST=0.0.0.0
PORT=8000

# CORS Configuration
# Use "*" for development, specific origins for production
CORS_ORIGINS=*
